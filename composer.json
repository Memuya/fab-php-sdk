{
    "name": "memuya/fab-php-sdk",
    "description": "A library to query and filter a list of cards and communicate with external APIs.",
    "authors": [
        {
          "name": "Mehmet Uyanik",
          "email": "mehmet.uyanik@live.com.au"
        }
    ],
    "require": {
        "php": ">=8.2",
        "league/flysystem": "^3.0",
        "league/flysystem-memory": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.5",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
          "Memuya\\Fab\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "fix-dry": "php-cs-fixer fix --dry-run --diff",
        "fix": "php-cs-fixer fix",
        "test": "phpunit tests/"
    }
}
